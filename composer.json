{
  "name": "wouaka/wouaka-sdk",
  "description": "SDK PHP officiel pour l'API Wouaka - Solutions de scoring crédit et vérification KYC pour l'Afrique de l'Ouest",
  "type": "library",
  "keywords": [
    "wouaka",
    "kyc",
    "verification",
    "credit-scoring",
    "iea",
    "uemoa",
    "africa",
    "trustlink",
    "fintech",
    "microfinance"
  ],
  "homepage": "https://wouaka-creditscore.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Wouaka Team",
      "email": "contact@wouaka-creditscore.com",
      "homepage": "https://wouaka-creditscore.com"
    }
  ],
  "support": {
    "email": "support@wouaka-creditscore.com",
    "docs": "https://docs.wouaka-creditscore.com",
    "issues": "https://github.com/fofanay/wouaka-php-sdk/issues"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Wouaka\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wouaka\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src --level=5",
    "cs": "phpcs --standard=PSR12 src tests",
    "cs-fix": "phpcbf --standard=PSR12 src tests"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
